@page "/"

@using System.Xml.Linq
@using AMS2aiGenerator.Models
@using Microsoft.AspNetCore.Mvc
@using System.Xml

@inject IJSRuntime JSRuntime

<PageTitle>AMS2 </PageTitle>


    <EditForm Model=@Person>
        <RadzenCard Style="margin:20px">
            <div class="col-md-10">

            <RadzenDropDown AllowClear="true" TValue="string" AllowVirtualization="true"
             AllowFiltering="true" @bind-Value="@Person.livery_name" Placeholder="livery_name"
             Data=@presets.liveries FilterCaseSensitivity="FilterCaseSensitivity.CaseInsensitive" />
            <RadzenTextBox @bind-Value=Person.name Placeholder="Name" />
            <RadzenTextBox @bind-Value=Person.country Placeholder="Country" />
            </div>
            <div class="col-md-2">
                <RadzenLabel Text="race skill" />
                @Person.race_skill
                <RadzenSlider @bind-Value=@Person.race_skill TValue="float" Min="0" Max="1" Step="0.01" />

                <RadzenLabel Text="qualifying skill " />
                @Person.qualifying_skill
                <RadzenSlider @bind-Value=@Person.qualifying_skill TValue="float" Min="0" Max="1" Step="0.01" />
            <input type="submit" value="Add" onclick="@SaveDriver" class="btn btn-primary" />
            </div>
          
      
                </RadzenCard>
    </EditForm>




@if (driverListXML != null)
{
    <DriverTable driverList="@driverList"/>
}


